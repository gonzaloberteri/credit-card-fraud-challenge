{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint --ext .ts .",
    "lint:fix": "eslint --ext .ts --fix .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen": "npm run codegen",
    "codegen": "npm run codegen:tender && npm run codegen:contentful",
    "codegen:tender": "graphql-codegen --config codegen.ts --project tender",
    "codegen:contentful": "graphql-codegen --config codegen.ts --project contentful",
    "predev": "yarn generate",
    "prepare": "if [[ -x \"$(command -v husky)\" ]]; then cd .. && husky install api/.husky; fi",
    "precommit": "lint-staged && yarn tsc",
    "prepush": "yarn lint && yarn tsc",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write 'src/**/*.ts'"
  },
  "dependencies": {
    "@amityco/ts-sdk": "^6.5.2",
    "@apple/app-store-server-library": "^1.1.0",
    "@contentful/rich-text-types": "15.13.2",
    "@golevelup/nestjs-graphql-request": "^0.1.13",
    "@google-cloud/documentai": "^8.2.0",
    "@google-cloud/logging-winston": "^5.3.0",
    "@google-cloud/storage": "^6.8.0",
    "@google-cloud/tasks": "^4.0.1",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.1",
    "@react-pdf/renderer": "^3.4.4",
    "@sendgrid/mail": "^7.7.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "analytics-node": "^6.2.0",
    "axios": "^1.4.0",
    "cities": "^2.0.0",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "decorate-all": "^1.2.1",
    "degit": "^2.8.4",
    "firebase-admin": "^11.2.1",
    "geo-tz": "^8.0.2",
    "graphql": "^16.6.0",
    "graphql-request": "^3.5.0",
    "graphql-tag": "^2.12.6",
    "handlebars": "^4.7.7",
    "http-proxy-middleware": "^3.0.0",
    "https": "^1.0.0",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.0",
    "launchdarkly-node-server-sdk": "^7.0.3",
    "mixpanel": "^0.18.0",
    "nest-winston": "^1.10.0",
    "nestjs-cls": "^3.3.1",
    "node-gyp": "^10.0.1",
    "nylas": "^6.8.0",
    "openai": "^4.28.0",
    "qs": "^6.11.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rrule": "^2.8.1",
    "rxjs": "^7.2.0",
    "twilio": "^3.83.4",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/analytics-node": "^3.1.9",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsc": "^2.0.0",
    "husky": "^8.0.2",
    "jest": "28.1.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "resolutions": {
    "@types/mime": "3.0.4"
  },
  "lint-staged": {
    ".{ts,graphql}": [
      "eslint --fix --max-warnings=0 --ignore-path '.eslintignore'"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
